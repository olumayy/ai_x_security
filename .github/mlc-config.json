{
  "ignorePatterns": [
    {
      "pattern": "^https://colab.research.google.com"
    },
    {
      "pattern": "^https://github.com/.*/blob/"
    },
    {
      "pattern": "localhost"
    },
    {
      "pattern": "127.0.0.1"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://api.github.com"],
      "headers": {
        "Accept": "application/vnd.github.v3+json"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308]
}
